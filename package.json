{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon ./bin/www",
    "dev": "concurrently --kill-others-on-fail \"npm run-script server\" \"npm run-script client\"",
    "lint": "eslint",
    "start": "node  ./bin/www",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "mongodb": "^3.1.8",
    "morgan": "~1.9.0",
    "promise-retry": "^1.1.1"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.4"
  }
}
